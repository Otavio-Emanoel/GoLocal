      },
      "edgeToEdgeEnabled": true,
      "package": "com.otavioemanoel.GoLocal"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-mail-composer"
    ],
    "extra": {
      "eas": {
        "projectId": "b1599e20-a552-48e0-8578-1605f2d07e4f"
      }
    }
  }
}
